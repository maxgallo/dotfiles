{
    "codeLens.enable": true,
    "diagnostic.errorSign": "✘",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.signOffset": 9999999,
    "eslint.enable": true,
    "eslint.filetypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.nodePath": "/usr/local/lib/node_modules",
    "git.realtimeGutters": true,
    "git.enableGutters": false,
    "tsserver.enableJavascript": true,
    "java.home": "/Library/Java/JavaVirtualMachines/openjdk.jdk/Contents/Home/",
    "java.trace.server": "verbose",
    "java.format.enabled": true,
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": true,
    "yaml.completion": true,
    "yaml.schemas": {
        "https://s3.eu-central-1.amazonaws.com/schemas.plat.dazn-dev.com/dazn-manifest/3.3.0.schema.json": [".dazn-manifest.yml"]
    },
    "yaml.schemaStore.enable": true
}
